{
  "name": "packing-list",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"tsx watch src/server/index.ts\"",
    "build": "vite build && tsup src/server/index.ts --format esm --out-dir dist/server",
    "start": "node dist/server/index.js",
    "typecheck": "tsc --noEmit",
    "preview": "npm run build && npm start"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "better-sqlite3": "^12.2.0",
    "express": "^5.1.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/express": "^5.0.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.0",
    "tailwindcss": "^4.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^7.3.1"
  }
}
